{
  "name": "b-admin-template",
  "description": "一个后台管理系统Web项目",
  "version": "1.0.3",
  "author": "bbnocode",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=8.7.1"
  },
  "license": "MIT",
  "lint-staged": {
    "*.{js,ts,vue,tsx,jsx}": "eslint"
  },
  "config": {
    "commitizen": {
      "path": "cz-vinyl"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bibinocode/vub-admin"
  },
  "keywords": [
    "Vue3",
    "Vite",
    "Typescript",
    "element-plus",
    "vue-admin"
  ],
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "vite --mode base",
    "build:prod": "vite build --mode pro",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "g": "plop",
    "npm-check": "pnpx npm-check -u",
    "clean": "pnpm exec rimraf node_modules",
    "cz": "pnpm git cz",
    "in": "pnpm install",
    "prepare": "husky",
    "log-tag": "pnpm exec conventional-changelog -p ./changelog.config.js -i CHANGELOG.md -s -r 0 --tag-prefix 'v'",
    "log": "pnpm exec conventional-changelog -p ./changelog.config.js -i CHANGELOG.md -s --skip-unreleased",
    "clean-cache": "pnpm exec rimraf node_modules/.cache",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@unhead/vue": "^1.11.6",
    "@vueuse/core": "^11.1.0",
    "axios": "^1.7.7",
    "inquirer": "8",
    "install": "^0.13.0",
    "json-bigint": "^1.0.0",
    "pinia": "^2.2.2",
    "swiper": "^11.1.14",
    "vue": "^3.5.8",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@eslint/js": "^9.11.1",
    "@iconify/json": "^2.2.252",
    "@rushstack/eslint-patch": "^1.10.4",
    "@tsconfig/node20": "^20.1.4",
    "@types/json-bigint": "^1.0.4",
    "@types/node": "^22.6.2",
    "@unocss/eslint-config": "^0.62.4",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vitejs/plugin-vue-jsx": "^4.0.1",
    "@vue-macros/volar": "^0.30.2",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/tsconfig": "^0.5.1",
    "commitizen": "^4.3.0",
    "conventional-changelog": "^6.0.0",
    "conventional-changelog-cli": "^5.0.0",
    "cz-vinyl": "^2.5.1",
    "eslint": "^9.11.1",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-import-helpers": "^1.3.1",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-vue": "^9.28.0",
    "husky": "^9.1.6",
    "lightningcss": "^1.27.0",
    "lint-staged": "^15.2.10",
    "markdown-it-prism": "^2.3.0",
    "npm-check": "^6.0.1",
    "npm-run-all2": "^6.2.3",
    "plop": "^4.0.1",
    "prettier": "^3.2.5",
    "rimraf": "^6.0.1",
    "sass": "^1.79.3",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.7.0",
    "unocss": "^0.62.4",
    "unplugin-auto-import": "^0.18.3",
    "unplugin-icons": "^0.19.3",
    "unplugin-vue-components": "^0.27.4",
    "unplugin-vue-macros": "^2.12.2",
    "unplugin-vue-markdown": "^0.26.2",
    "unplugin-vue-router": "^0.10.8",
    "vite": "^5.4.7",
    "vite-plugin-cdn-import": "^1.0.1",
    "vite-plugin-pwa": "^0.20.5",
    "vite-plugin-vue-devtools": "^7.4.6",
    "vite-plugin-vue-layouts": "^0.11.0",
    "vue-eslint-parser": "^9.4.3",
    "vue-tsc": "^2.1.6"
  }
}
